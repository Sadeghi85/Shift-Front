{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "vuejs: chrome",
      "url": "http://127.0.0.1:8080",
      "webRoot": "${workspaceFolder}",
      //"breakOnLoad": true,
      "sourceMapPathOverrides": {
        "webpack:///./*": "${webRoot}/*"
      },
      "skipFiles": ["${workspaceFolder}/node_modules/**/*"],
      //   "pathMapping": {
      //     "/_karma_webpack_": "${workspaceFolder}"
      //   },
      //   "sourceMapPathOverrides": {
      //     "webpack:/*": "${webRoot}/*",
      //     "/./*": "${webRoot}/*",
      //     "/src/*": "${webRoot}/*",
      //     "/*": "*",
      //     "/./~/*": "${webRoot}/node_modules/*"
      //   },
      "preLaunchTask": "vuejs: start"
    }
  ]
}
